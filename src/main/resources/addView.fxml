<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.101" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monitoringsystemturbo.presenter.AddApplicationPresenter">
    <center>
        <GridPane hgap="5.0" vgap="5.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints maxWidth="207.0" minWidth="66.0" prefWidth="97.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="229.0" minWidth="10.0" prefWidth="198.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" />
                <RowConstraints minHeight="10.0" />
            <RowConstraints />
            </rowConstraints>
           <children>
               <Label text="Name" GridPane.rowIndex="0">
                   <GridPane.margin>
                       <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                   </GridPane.margin>
               </Label>
               <Label text="Full path" GridPane.rowIndex="1">
                   <GridPane.margin>
                       <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                   </GridPane.margin>
               </Label>
               <TextField fx:id="nameApplication" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="0">
                   <GridPane.margin>
                       <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                   </GridPane.margin>
               </TextField>
               <TextField fx:id="fullPathApplication" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1">
                   <GridPane.margin>
                       <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
                   </GridPane.margin>
               </TextField>
           </children>

        </GridPane>
    </center>
    <bottom>
        <HBox alignment="BOTTOM_RIGHT" prefHeight="46.0" prefWidth="290.0" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" onAction="#onCancel" prefHeight="31.0" prefWidth="76.0" text="Cancel">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="addButton" mnemonicParsing="false" disable="true" onAction="#onAdd" prefWidth="60.0" text="Add">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin></Button>
                <Button fx:id="findButton" mnemonicParsing="false" onAction="#onFind" prefWidth="60.0" text="Find">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin></Button>
            </children>
            <BorderPane.margin>
                <Insets bottom="0.0" left="5.0" right="5.0" top="5.0" />
            </BorderPane.margin>
        </HBox>
    </bottom>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</BorderPane>
